<div class="page type-page status-publish hentry clearfix post nodate">
	<div class="entry clearfix">                
		<h1 class="post-title entry-title">Site Feedback</h1>
		<div class="entry-content clearfix">
		
<form name="main" method="POST">
<input type="hidden" name="action" value="main">
<input type="hidden" name="function" value="main_feedback_save">

<h1 class="post-title entry-title">Site Feedback</h1>

<table width="100%" cellpadding="2" cellspacing="1" class="tableborder">
{if $user.user_id==0}
<tr>
	<th valign="top">Email Address</th>
	<td>
		<input type="text" size="50" name="email_address">
	</td>
</tr>
{/if}
<tr>
	<th valign="top">Feedback, Comments, or Feature Requests</th>
	<td>
		<textarea cols="75" rows="8" name="feedback_string"></textarea>
	</td>
</tr>
</table>
<center>
<br>
<input type="submit" value=" Send This Feedback " class="block-button">
</center>
</form>
